<odoo>
    <data>
         <record model="ir.module.category" id="check_category">
            <field name="name">check</field>
            <field name="description">Helps you manage your check-related processes</field>
            <field name="sequence">10</field>
        </record>

        <record id="group_check_user" model="res.groups">
            <field name="name">User</field>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
            <field name="category_id" ref="mazaya_accounting.check_category"/>
        </record>

        <record id="group_check_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="mazaya_accounting.check_category"/>
            <field name="implied_ids" eval="[(4, ref('mazaya_accounting.group_check_user'))]"/>
        </record>


    </data>
</odoo>